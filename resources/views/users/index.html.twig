{% extends 'components/layout.html.twig' %}

{% block title %}Users{% endblock %}

{% block content %}
  <h1>Users</h1>
  <p>This is the users page.</p>

  <ul>
    {% for user in users %}
      <li><a href='users/{{ user.getId() }}'>
        {{ user.getName() }} - {{ user.getEmail() }}
      </a></li>
    {% endfor %}
  </ul>
{% endblock %}
